  "devDependencies": {
    "concurrently": "^7.0.0"
  },
  "bugs": {
    "url": "https://github.com/wi1dcode/hack2paris/issues"
  },
  "homepage": "https://github.com/wi1dcode/hack2paris#readme",
  "dependencies": {
    "setup": "^0.0.3"
  }
}
